{
  "name": "ukraine-war-bot",
  "version": "0.0.1",
  "description": "Discord bot for notifying discord users with latest news on war ",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "swc src -d dist",
    "build-dev": "pnpm run build && cross-env NODE_ENV=development node --inspect -r dotenv/config dist/index.js",
    "dev": "nodemon --ext ts --exec \"pnpm run build-dev\"",
    "start": "pnpm run build && cross-env NODE_ENV=production node -r dotenv/config dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix"
  },
  "keywords": [
    "js",
    "discord"
  ],
  "author": "Mario",
  "license": "ISC",
  "devDependencies": {
    "@swc-node/core": "^1.8.2",
    "@swc-node/register": "^1.4.2",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.146",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.10.0",
    "nodemon": "^2.0.15",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/rest": "^0.3.0",
    "discord-api-types": "^0.27.3",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "mongoose": "^6.2.3",
    "regenerator-runtime": "^0.13.9"
  },
  "engines": {
    "node": "16.14.0"
  }
}
